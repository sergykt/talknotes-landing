{
  "extends": ["stylelint-config-standard-scss", "stylelint-config-tailwindcss/scss"],
  "rules": {
    "property-no-vendor-prefix": null,
    "media-feature-range-notation": "prefix",
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["use", "forward", "tailwind"]
      }
    ]
  }
}
